prefix=@prefix@
exec_prefix=@exec_prefix@

if test "@includedir@" != /usr/include ; then
  LIBGLADE_INCLUDEDIR="-I@includedir@ @XML_CFLAGS@"
else
  LIBGLADE_INCLUDEDIR="@XML_CFLAGS@"
fi
LIBGLADE_LIBDIR="-L@libdir@ @GNOME_LIBDIR@"
LIBGLADE_LIBS="-lglade-gnome -lglade @XML_LIBS@ @GNOMEUI_LIBS@"
MODULE_VERSION="libglade-@VERSION@"

